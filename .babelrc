{
  "presets": [
    ["env",
      {
        "targets": {
          "browsers": [
            "last 2 versions",
            "ie >= 7"
          ]
        },
        "modules": false,
        "useBuiltIns": false,
        "debug": false
      }
    ]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-es2015-modules-umd"
  ]
}
